msgid "Language &amp; Region Settings"
msgstr "Paramètres de langue et de région"

#: data/locale.metainfo.xml.in:11
msgid "Manage languages, and configure region and format"
msgstr "Gérer les langues, configurer les paramètres régionaux"

#: data/locale.metainfo.xml.in:26
msgid "elementary, Inc."
msgstr "elementary, Inc."

#: data/locale.metainfo.xml.in:34 data/locale.metainfo.xml.in:44
msgid "Minor improvements:"
msgstr "Améliorations mineures :"

#: data/locale.metainfo.xml.in:36
msgid "More responsive design layout"
msgstr "Mise en page plus réactive"

#: data/locale.metainfo.xml.in:37 data/locale.metainfo.xml.in:49
#: data/locale.metainfo.xml.in:64 data/locale.metainfo.xml.in:117
msgid "Updated translations"
